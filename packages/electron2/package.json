{
  "name": "@mapwatch/electron2",
  "version": "1.0.5",
  "main": "src/main.js",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "start": "bash -c \"electron . --spawn_www --app_url=http://localhost:3000\"",
    "build": "electron-builder --win --x64",
    "release": "yarn build --publish always"
  },
  "build": {
    "appId": "org.erosson.mapwatch",
    "productName": "Mapwatch",
    "artifactName": "${productName}-${version}.${ext}",
    "files": [
      "node_modules/",
      "src/",
      "package.json"
    ],
    "appImage": {},
    "win": {
      "icon": "src/favicon.png"
    },
    "publish": [
      {
        "owner": "mapwatch",
        "repo": "mapwatch",
        "provider": "github"
      }
    ],
    "directories": {
      "output": "dist/"
    }
  },
  "dependencies": {
    "chokidar": "^3.3.1",
    "electron-log": "^4.1.1",
    "electron-updater": "^4.2.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "regedit": "^3.0.3"
  },
  "devDependencies": {
    "electron": "13.1.7",
    "electron-builder": "^22.7.0"
  }
}
